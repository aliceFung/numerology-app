<h3>People</h3>
<table class="table table-striped">
    <thead>
    <th>Name</th>
    <th>Birthdate</th>
    </thead>
    <tbody>
    <% @people.each do |person| %>
        <tr>
            <td>
                <a href= "<%= "/people/#{person.id}"%>">
            <%= "#{person.first_name} #{person.last_name}" %>
    </a>
            </td>
            <td>
            <%= "#{person.birthdate.strftime("%F")}" %>
            </td>
        </tr>
    <% end %>
    </tbody>
</table>
